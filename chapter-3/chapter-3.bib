	issn = {00280836},
	journal = {Nature},
	number = {7082},
	pages = {297--302},
	pmid = {16541064},
	primaryClass = {cond-mat},
	title = {{Folding DNA to create nanoscale shapes and patterns}},
	volume = {440},
	year = {2006}
}
