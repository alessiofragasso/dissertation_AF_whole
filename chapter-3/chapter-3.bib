	title = {{Electrolytic transport through a synthetic nanometer-diameter pore}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/16020525{\%}5Cnhttp://www.pnas.org/cgi/doi/10.1073/pnas.0500796102},
