@article{Naim2009,
	abstract = {To fulfil their function, nuclear pore complexes (NPCs) must discriminate between inert proteins and nuclear transport receptors (NTRs), admitting only the latter. This specific permeation is thought to depend on interactions between hydrophobic patches on NTRs and phenylalanine-glycine (FG) or related repeats that line the NPC. Here, we tested this premise directly by conjugating different hydrophobic amino-acid analogues to the surface of an inert protein and examining its ability to cross NPCs unassisted by NTRs. Conjugation of as few as four hydrophobic moieties was sufficient to enable passage of the protein through NPCs. Transport of the modified protein proceeded with rates comparable to those measured for the innate protein when bound to an NTR and was relatively insensitive both to the nature and density of the amino acids used to confer hydrophobicity. The latter observation suggests a non-specific, small, and plant interaction network between cargo and FG repeats.},
	author = {Naim, Bracha and Zbaida, David and Dagan, Shlomi and Kapon, Ruti and Reich, Ziv},
	doi = {10.1038/emboj.2009.225},
	file = {:Users/localadmin/Desktop/Biomimetic{\_}NPC{\_}project/Characterization/emboj2009225a.pdf:pdf},
	issn = {02614189},
	journal = {EMBO Journal},
	keywords = {Hydrophobicity,Nuclear pore complex,Selectivity,Transport},
	number = {18},
	pages = {2697--2705},
	publisher = {Nature Publishing Group},
	title = {{Cargo surface hydrophobicity is sufficient to overcome the nuclear pore complex selectivity barrier}},
