@article{RalphM.M.Smeets2005,
	abstract = {We report experimental measurements of the salt dependence of ion transport and DNA translocation through solid-state nanopores. The ionic conductance shows a three-order-of-magnitude decrease with decreasing salt concentrations from 1 M to 1 $\mu$M, strongly deviating from bulk linear behavior. The data are described by a model that accounts for a salt-dependent surface charge of the pore. Subsequently, we measure translocation of 16.5-$\mu$m-long dsDNA for 50 mM to 1 M salt concentrations. DNA translocation is shown to result in either a decrease ([KCl] {\textgreater} 0.4 M) or increase of the ionic current ([KCl] {\textless} 0.4 M). The data are described by a model where current decreases result from the partial blocking of the pore and current increases are attributed to motion of the counterions that screen the charge of the DNA backbone. We demonstrate that the two competing effects cancel at a KCl concentration of 370 ± 40 mM.},
	author = {{Ralph M. M. Smeets} and {Ulrich F. Keyser} and {Diego Krapf} and {Meng-Yue Wu} and {Nynke H. Dekker}, And and Dekker*, Cees},
	doi = {10.1021/NL052107W},
	file = {:Users/localadmin/Desktop/Biomimetic{\_}NPC{\_}project/Nanopores/smeets2006.pdf:pdf},
	journal = {Nano Letters},
	number = {1},
	title = {{Salt Dependence of Ion Transport and DNA Translocation through Solid-State Nanopores}},
	url = {http://pubs.acs.org/doi/abs/10.1021/nl052107w?journalCode=nalefd{\&}quickLinkVolume=6{\&}quickLinkPage=89{\&}selectedTab=citation{\&}volume=6},
	volume = {6},
	year = {2005}
}
