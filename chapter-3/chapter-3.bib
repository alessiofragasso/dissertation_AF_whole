	journal = {Cell Calcium},
	keywords = {Calcium channels,Confocal microscopy,Single-channel imaging,TIRF microscopy,Xenopus oocyte},
	number = {4},
	pages = {283--299},
	title = {{Optical single-channel recording by imaging Ca2+ flux through individual ion channels: Theoretical considerations and limits to resolution}},
	volume = {37},
	year = {2005}
}
